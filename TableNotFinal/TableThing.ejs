<script>
    //in minutes
    var fromTime = 1020
    var toTime = 1260

    var currentTime = new Date();
    var time1 = currentTime.getHours() * 60 + currentTime.getMinutes();
    if (time1 < fromTime) {
        document.getElementById("timeL").style.top = "18%";
    }
    else if (time1 > toTime) {
        document.getElementById("timeL").style.top = "100%"
    }
    else {
        document.getElementById("timeL").style.top = ((time1 - fromTime) / (toTime - fromTime) * 92 + 18 + "%")
        if (time1 > 1020 && time1 < 1050) { document.getElementById("2:1700").style.backgroundColor = "lightgrey" } else { document.getElementById("2:1700").style.backgroundColor = "white" }
