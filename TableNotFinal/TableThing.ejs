<script>
    var numofelements = 7;// שים לב לשנות לפי מספר האלמנטים
    var centerId = 2;

    function move(a)
    {
        var i = 0;
        function myfor()
        {
            setTimeout(function () {
                for (var i2 = 0; i2 < numofelements; i2++)
                {
                    var tempP = document.getElementById(parseInt(numofelements) - i2).style.left;
                    tempP = (parseInt(tempP.substring(0, tempP.length - 1)) + a) + "%";
                    document.getElementById(parseInt(numofelements) - i2).style.left = tempP;
                }
                i++;
                if (i < 100)
                {
                    myfor();
                }
            }, 1)
        }
        myfor();
    }



    function right()
    {
        if (centerId != numofelements)
        {
            centerId++;
            move(-1);
        }
        else
        {
            //what you want to do if it fails havnt decided yet
        }
    }




    function left()
    {
        if (centerId != 1)
        {
            centerId--;
            move(1);
        }
        else
        {
            //what you want to do if it fails havent decided yet
        }
    }


</script >




